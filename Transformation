 #p1
    {
      transfrom: translateX(50px);
      transfrom: translateY(50px);
      transfrom: translate(50px,50px);    /*det här som de första x och y och det blir = 50px for x och 50px for y*/
      transfrom: rotateX(180deg);
      transfrom: rotateY(180deg);
      transfrom: rotateZ(180deg);
      transfrom: rotate(180deg);
      transfrom: scaleX(2);
      transfrom: scaleY(0.5);
      transfrom: scale(2,2);        /*det här är scale X och Y tillsammans*/
      transfrom: skewY(45deg);
      transfrom: skewX(45deg);
      
      transform: matrix(1,0,0,1,0,0);   /* ettorna är stander alltså för att fuguren ska syns*/
      
