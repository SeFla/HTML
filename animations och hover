// typsnitt
#p1
{
  width: 100px;
  height: 100px; 
  background: red;
  font-size: 85px;
  text-align: center;
  margin: 0px;
  
  animation: myFont;        
  animation-duration: 5s;
  animation-iteration-count: infinite;    
}

@keyframes  myColorAnimation        
{
    0%{background-color:red;}                   
    20%{background-color:orange;}
    40%{background-color:yellow;}
    60%{background-color:green;}
    80%{background-color:blue;}
    100%{background-color:purple;}
}

/*@keyframes myRotateAnmation
         
{
    100%{transform: rotateZ(360deg)}  
              
               
}
*/
@keyframes  myTranslate
{
    50% {transform: translate(0px,100px);}

}

@keyframes myScale                   // scale animation 
{
  50%{transform: scale(3.2)}        // siffrna för transform

}

@keyframes myOpacity
{
    0%{opacity:0;}
    50%{opacity:1;}
    100%{opacity:0;}
}

@keyframes myFont           // font animation 
{
  0%{font-size:85px;}
  50%{font-size:400px;}
  100%{font-size:85px;}

}
________________________________________________________________________________________________
// hover 

#p1
{
  width: 100px;
  height: 100px; 
  background: red;
  font-size: 85px;
  text-align: center;
  margin: 0px;
  
    
}
#p1: hover                    // hover 
{
      animation: myFont;        
      animation-duration: 5s;
      animation-iteration-count: infinite;

}

@keyframes  myColorAnimation        
{
    0%{background-color:red;}                   
    20%{background-color:orange;}
    40%{background-color:yellow;}
    60%{background-color:green;}
    80%{background-color:blue;}
    100%{background-color:purple;}
}

/*@keyframes myRotateAnmation
         
{
    100%{transform: rotateZ(360deg)}  
              
               
}
*/
@keyframes  myTranslate
{
    50% {transform: translate(0px,100px);}

}

@keyframes myScale                   // scale animation 
{
  50%{transform: scale(3.2)}        // siffrna för transform

}

@keyframes myOpacity
{
    0%{opacity:0;}
    50%{opacity:1;}
    100%{opacity:0;}
}

@keyframes myFont           // font animation 
{
  0%{font-size:85px;}
  50%{font-size:400px;}
  100%{font-size:85px;}

}
